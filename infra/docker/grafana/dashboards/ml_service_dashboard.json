{
  "uid": "ml-service-metrics",
  "title": "ML Service Metrics",
  "tags": ["ml", "rating"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "annotations": {"list": []},
  "time": {"from": "now-30m", "to": "now"},
  "timepicker": {},
  "templating": {"list": []},
  "panels": [
    {
      "id": 1,
      "title": "Inference Latency (P95)",
      "type": "timeseries",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(ml_inference_latency_seconds_bucket{endpoint=\"rate_script\"}[5m]))",
          "legendFormat": "p95 latency"
        }
      ]
    },
    {
      "id": 2,
      "title": "Request Rate",
      "type": "timeseries",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(ml_requests_total{status=\"success\"}[5m])",
          "legendFormat": "Success"
        },
        {
          "expr": "rate(ml_requests_total{status=\"error\"}[5m])",
          "legendFormat": "Error"
        }
      ]
    },
    {
      "id": 3,
      "title": "Active Requests",
      "type": "timeseries",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 8, "w": 8, "h": 8},
      "targets": [
        {
          "expr": "ml_active_requests",
          "legendFormat": "Active requests"
        }
      ]
    },
    {
      "id": 4,
      "title": "Average Violence Score",
      "type": "timeseries",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 8, "y": 8, "w": 8, "h": 8},
      "targets": [
        {
          "expr": "ml_avg_violence_score",
          "legendFormat": "Violence"
        }
      ]
    },
    {
      "id": 5,
      "title": "Rating Distribution",
      "type": "piechart",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 16, "y": 8, "w": 8, "h": 8},
      "targets": [
        {
          "expr": "ml_rating_distribution",
          "legendFormat": "{{rating}}"
        }
      ]
    },
    {
      "id": 6,
      "title": "Scenes Per Script",
      "type": "timeseries",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(ml_scenes_per_script_sum[5m]) / rate(ml_scenes_per_script_count[5m])",
          "legendFormat": "Avg scenes"
        }
      ]
    },
    {
      "id": 7,
      "title": "Content Feature Scores",
      "type": "timeseries",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
      "targets": [
        {"expr": "ml_avg_violence_score", "legendFormat": "Violence"},
        {"expr": "ml_avg_sex_score", "legendFormat": "Sex"},
        {"expr": "ml_avg_gore_score", "legendFormat": "Gore"},
        {"expr": "ml_avg_profanity_score", "legendFormat": "Profanity"},
        {"expr": "ml_avg_drugs_score", "legendFormat": "Drugs"}
      ]
    }
  ]
}
